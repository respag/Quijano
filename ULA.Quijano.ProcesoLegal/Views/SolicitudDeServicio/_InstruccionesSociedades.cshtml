@{
    Layout = null;
}

<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title text-center">Datos Generales</h3>
    </div>
    <div class="panel-body">

        <div role="tabpanel">

            <!-- Nav tabs -->
            <ul class="nav nav-tabs pestanias" role="tablist">

                @if (ViewBag.Step == "RegistrarSolicitud" || ViewBag.Step == "AnalisisLegal" || ViewBag.Step == "InstruccionAbogadoGestor" || ViewBag.Step == "EjecutarInstruccion" || ViewBag.Step == "Entrega")
                {
                    <li role="presentation" class="active">
                        <a href="#home" aria-controls="home" role="tab" data-toggle="tab" class="nav-active">Datos de la Sociedad</a>
                    </li>
                }

                @if (ViewBag.Step == "RegistrarSolicitud" || ViewBag.Step == "AnalisisLegal" || ViewBag.Step == "InstruccionAbogadoGestor" || ViewBag.Step == "EjecutarInstruccion" || ViewBag.Step == "Entrega")
                {
                    <li role="presentation">
                        <a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Datos de Facturación</a>
                    </li>
                }

                @if (ViewBag.Step == "EjecutarInstruccion" || ViewBag.Step == "Monitoreo")
