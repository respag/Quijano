                                        <option></option>
                                        <option value="PA">Nombre 1</option>
                                        <option value="CT">Nombre 2</option>
                                        <option value="NY">Nombre 3</option>
                                    </select>
                                </div>
                            </div>
                                    *@
                                    <div class="col-xs-4">
                                        <div class="input-group padding-bottom form-inline input-group-sm">
                                            <span class="input-group-addon" id="basic-addon1"><span class="glyphicon glyphicon-usd" aria-hidden="true"></span>&nbsp;&nbsp;Monto Cap:</span>
                                            <input id="txtDatSocMontoCap" name="txtDatSocMontoCap" type="text" class="form-control" aria-describedby="basic-addon1" required />
                                        </div>
