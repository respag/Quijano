                    <li role="presentation" @if (ViewBag.Step == "Monitoreo") { @("class=active") } >
                        <a href="#asuntos" aria-controls="abrirAsuntos" role="tab" data-toggle="tab" @if (ViewBag.Step == "Monitoreo") { @("class=nav-active") } >Abrir Asunto</a>
                    </li>
                }

                @if (ViewBag.Step == "Control")
